            <kind>1</kind>
            <string>/// Dequeue

if (must_dequeue){
    
    // If there are events in the queue
    if (mat_event_queue_size() &gt; 0){
        
        // Get the first event
        var next_event = mat_event_queue_pop();
    
        // If the event object exists
        if (instance_exists(next_event)){
        
            must_dequeue = false;
            
            with(next_event){
                // If the event does not have a delay
                if (delay == 0){
                    // Fire the event immediately
                    event_user(0);
                }
                else
                if (delay &gt; 0){
                    // Set an alarm to fire the event
                    alarm[10] = delay;
                }
            }
        }
    }
}
